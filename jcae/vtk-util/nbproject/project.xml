<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.jcae.vtk</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>gnu.trove</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>javax.vecmath</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>vtk</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>org.jcae.vtk</package>
                <package>org.jcae.geometry</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/vtk-util.jar</runtime-relative-path>
                <binary-origin>${cluster}/modules/ext/vtk-util.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
